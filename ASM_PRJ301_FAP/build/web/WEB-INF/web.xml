<?xml version="1.0" encoding="UTF-8"?>
<web-app version="6.0" xmlns="https://jakarta.ee/xml/ns/jakartaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="https://jakarta.ee/xml/ns/jakartaee https://jakarta.ee/xml/ns/jakartaee/web-app_6_0.xsd">
    <servlet>
        <servlet-name>login</servlet-name>
        <servlet-class>controller.LoginController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>timeTable</servlet-name>
        <servlet-class>controller.TimeTableController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>attendance</servlet-name>
        <servlet-class>controller.AttendanceController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>InformationGroupController</servlet-name>
        <servlet-class>controller.InformationGroupController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>PointController</servlet-name>
        <servlet-class>controller.PointController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AttendanceViewByStudent</servlet-name>
        <servlet-class>controller.AttendanceViewByStudent</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LogoutController</servlet-name>
        <servlet-class>controller.LogoutController</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>login</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>timeTable</servlet-name>
        <url-pattern>/timeTable</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>attendance</servlet-name>
        <url-pattern>/attendance</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>InformationGroupController</servlet-name>
        <url-pattern>/checkGroup</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>PointController</servlet-name>
        <url-pattern>/point</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AttendanceViewByStudent</servlet-name>
        <url-pattern>/attViewStu</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LogoutController</servlet-name>
        <url-pattern>/logoutController</url-pattern>
    </servlet-mapping>
</web-app>
